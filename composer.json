{
    "name": "axy/codecs-base64vlq",
    "type": "library",
    "description": "Codec for VLQ (variable-length quantity) Base64 algorithm",
    "keywords": [
        "Codec",
        "VLQ",
        "Variable length quantity",
        "Base64",
        "Source map"
    ],
    "homepage": "https://github.com/axypro/codecs-base64vlq",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Grigoriev",
            "email": "go.vasac@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "axy/errors": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "axy\\codecs\\base64vlq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "axy\\codecs\\base64vlq\\tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist",
        "md": "phpmd --exclude vendor . text phpmd.xml.dist",
        "cs": "phpcs --standard=PSR2 --encoding=utf-8 --ignore=vendor ."
    },
    "archive": {
        "exclude": ["/tests/*", "/doc/*", "/cs.sh", "/phpmd.sh", "/*.xml.dist", "/CHANGELOG.md"]
    }
}
